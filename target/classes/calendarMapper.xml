<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeoung.project.calendar.CalendarMapper">
	<insert id="writeCalendar" parameterType="com.teamproject.teamproject.calendar.Calendar">
		insert into CALENDER (c_no, c_title, c_writer, c_txt, c_year, c_month, c_day, c_when) values (calender_seq.nextval,
		#{c_title}, #{c_writer}, #{c_txt}, #{c_year}, #{c_month}, #{c_day}, sysdate)
	</insert>
	<insert id="writePhotoCalendar" parameterType="com.teamproject.teamproject.calendar.Calendar">
		insert into CALENDER values (calender_seq.nextval, #{c_title}, #{c_writer}, #{c_txt}, #{c_photo},
		#{c_year}, #{c_month}, #{c_day}, sysdate)
	</insert>
	<select id="getCalendar" resultType="com.teamproject.teamproject.calendar.Calendar">
		select * from calender
	</select>
</mapper>